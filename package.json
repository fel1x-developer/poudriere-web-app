{
	"name": "poudriere",
	"version": "0.0.2",
	"description": "Poudriere Monitoring App",
	"keywords": [
		"poudriere",
		"freebsd",
		"svelte",
		"sveltekit",
		"daisyui",
		"typescript"
	],
	"homepage": "https://github.com/fel1x-developer/poudriere-web-app",
	"author": "Minsoo Choo <minsoochoo0122@proton.me>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fel1x-developer/poudriere-web-app.git"
	},
	"bugs": {
		"url": "https://github.com/fel1x-developer/poudriere-web-app/issues"
	},
	"type": "module",
	"license": "BSD-2-Clause",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"storybook": "STORYBOOK=true storybook dev -p 6006",
		"build-storybook": "STORYBOOK=true storybook build"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^3.2.6",
		"@eslint/compat": "^1.2.8",
		"@eslint/js": "^9.24.0",
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@playwright/test": "^1.51.1",
		"@storybook/addon-essentials": "^8.6.12",
		"@storybook/addon-interactions": "^8.6.12",
		"@storybook/addon-mdx-gfm": "^8.6.12",
		"@storybook/addon-svelte-csf": "^5.0.0-next.28",
		"@storybook/addon-themes": "^8.6.12",
		"@storybook/blocks": "^8.6.12",
		"@storybook/svelte": "^8.6.12",
		"@storybook/sveltekit": "^8.6.12",
		"@storybook/test": "^8.6.12",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.20.5",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.1.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"@types/node": "^22.14.0",
		"daisyui": "^5.0.19",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte": "^3.5.1",
		"globals": "^16.0.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"storybook": "^8.6.12",
		"svelte": "^5.25.12",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^4.1.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.29.1",
		"vite": "^6.2.6",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"@fortawesome/free-brands-svg-icons": "^6.7.2",
		"dotenv": "^16.5.0",
		"svelte-fa": "^4.0.3"
	}
}
